    "@babel/preset-typescript": "^7.6.0",
    "@types/chai": "^4.2.6",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.has": "^4.5.6",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.5",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@types/react-syntax-highlighter": "^11.0.2",
