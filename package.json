    "babel-preset-minify": "^0.5.1",
    "chai": "^4.2.0",
    "chain": "^0.1.3",
    "copy-webpack-plugin": "^5.0.4",
    "eslint": "^6.6.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^6.2.2",
    "pretty-format": "^24.9.0",
